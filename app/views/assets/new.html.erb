<h1>New asset for: <%= attachable_name %></h1>

<%= error_messages_for :asset %>

<% form_for(:asset, :url => user_assets_path(), :html => { :multipart => true }) do |f| %>

  <p>Select a file from your computer: <%= f.file_field :uploaded_data %></p>
  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', user_assets_path() %>
